{
  "[markdown]": {
    "editor.rulers": [
      85
    ],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 85,
    "editor.lineNumbers": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.minimap.enabled": false
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "cSpell.enabled": true
}
